<div id="googleMap" class="map"></div>
  <script>
  function initMap() {
  var dixie = {lat: 33.904428, lng: -84.486196};
  var map = new google.maps.Map(document.getElementById('googleMap'), {
    zoom: 15,
    disableDefaultUI: true,
    mapTypeId: google.maps.MapTypeId.HYBRID,
    center: dixie
  });
  var marker = new google.maps.Marker({
    position: dixie,
    map: map
    });
  }
  </script>
  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCUuzCHahqmivjlmPKJ3WF-xrqyLlgYz9M&callback=initMap"></script>
</div>
